{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime":  "automatic"
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-module-resolver",
      {
        "extensions": [".jsx", ".js"],
        "alias": {
          "src/(.+)": "./src/\\1"
        }
      }
    ]
  ]
}
